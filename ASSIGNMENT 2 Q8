// 8. Write a program to count the total number of distinct elements in an array of length n.
#include <iostream>
using namespace std;
int main(){
    int n;
    cout<<"Enter n: ";
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++) cin>>a[i];
    int count=0;
    for(int i=0;i<n;i++){
        int f=0;
        for(int j=0;j<i;j++){
            if(a[i]==a[j]){
                f=1;
                break;
            }
        }
        if(f==0) count++;
    }
    cout<<"Distinct elements = "<<count;
}
